---
# Apache Check Configuration
# Generated by Ansible - Do not edit manually

init_config:

instances:
{% for instance in datadog_checks_merged.apache.instances %}
  - apache_status_url: "{{ instance.apache_status_url | default('http://localhost/server-status') }}"
    apache_user: "{{ instance.apache_user | default('') }}"
    apache_password: "{{ instance.apache_password | default('') }}"
    {% if instance.tags is defined %}
    tags:
{% for tag in instance.tags %}
      - "{{ tag }}"
{% endfor %}
    {% endif %}
{% endfor %}
