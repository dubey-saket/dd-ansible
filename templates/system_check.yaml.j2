---
# System Check Configuration
# Generated by Ansible - Do not edit manually

init_config:

instances:
{% for instance in datadog_checks_merged.system.instances | default([{}]) %}
  {% if instance.collect_cpu_time is defined %}
  collect_cpu_time: {{ instance.collect_cpu_time | lower }}
  {% endif %}
  {% if instance.report_active is defined %}
  report_active: {{ instance.report_active | lower }}
  {% endif %}
  {% if instance.warning is defined %}
  warning: {{ instance.warning }}
  {% endif %}
  {% if instance.critical is defined %}
  critical: {{ instance.critical }}
  {% endif %}
  {% if instance.tags is defined %}
  tags:
{% for tag in instance.tags %}
    - "{{ tag }}"
{% endfor %}
  {% endif %}
{% endfor %}
