---
# Windows Service Check Configuration
# Generated by Ansible - Do not edit manually

init_config:

instances:
{% for instance in datadog_checks_merged.windows_service.instances %}
  - services:
{% for service in instance.services %}
      - "{{ service }}"
{% endfor %}
    {% if instance.tags is defined %}
    tags:
{% for tag in instance.tags %}
      - "{{ tag }}"
{% endfor %}
    {% endif %}
{% endfor %}
